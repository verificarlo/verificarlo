/* CTF 1.8 */

trace {
	major = 1;
	minor = 8;
	uuid = "b2d0cf7a-c945-408d-9dfb-c00c7051572e";
	byte_order = le;
	packet.header := struct {
		integer { size = 32; align = 8; signed = false; encoding = none; base = decimal; byte_order = native; } magic;
		integer { size = 8; align = 8; signed = false; encoding = none; base = decimal; byte_order = native; } uuid[16];
		integer { size = 32; align = 8; signed = false; encoding = none; base = decimal; byte_order = native; } stream_id;
	} align(8);
};

env {
	domain = "veritrace";
};

clock {
	name = my_clock;
	uuid = "2abf1261-27df-43ef-882c-cd2c123f42b3";
	description = "my clock";
	freq = 1000000000;
	precision = 1;
	offset_s = 0;
	offset = 0;
	absolute = false;
};

stream {
	id = 0;
	event.header := struct {
		integer { size = 32; align = 1; signed = false; encoding = none; base = decimal; byte_order = native; } id;
		integer { size = 64; align = 1; signed = false; encoding = none; base = decimal; byte_order = native; map = clock.my_clock.value; } timestamp;
	} align(1);

	packet.context := struct {
		integer { size = 64; align = 1; signed = false; encoding = none; base = decimal; byte_order = native; map = clock.my_clock.value; } timestamp_begin;
		integer { size = 64; align = 1; signed = false; encoding = none; base = decimal; byte_order = native; map = clock.my_clock.value; } timestamp_end;
		integer { size = 64; align = 1; signed = false; encoding = none; base = decimal; byte_order = native; map = clock.my_clock.value; } content_size;
		integer { size = 64; align = 1; signed = false; encoding = none; base = decimal; byte_order = native; map = clock.my_clock.value; } packet_size;
		integer { size = 64; align = 1; signed = false; encoding = none; base = decimal; byte_order = native; map = clock.my_clock.value; } events_discarded;
	} align(1);
};

event {
	name = "callpath";
	id = 0;
	stream_id = 0;
	fields := struct {
		integer { size = 64; align = 1; signed = false; encoding = none; base = decimal; byte_order = native; } parent;
		integer { size = 64; align = 1; signed = false; encoding = none; base = decimal; byte_order = native; } id;
		string { encoding = UTF8; } name;
	} align(8);
};

event {
	name = "context";
	id = 1;
	stream_id = 0;
	fields := struct {
		integer { size = 64; align = 1; signed = false; encoding = none; base = decimal; byte_order = native; } id;
		string { encoding = UTF8; } file;
		string { encoding = UTF8; } name;
		string { encoding = UTF8; } function;
		integer { size = 32; align = 1; signed = true; encoding = none; base = decimal; byte_order = native; } line;
		string { encoding = UTF8; } type;
	} align(8);
};

event {
	name = "value";
	id = 2;
	stream_id = 0;
	fields := struct {
		integer { size = 64; align = 1; signed = false; encoding = none; base = decimal; byte_order = native; } context;
		integer { size = 64; align = 1; signed = false; encoding = none; base = decimal; byte_order = native; } parent;
		floating_point { exp_dig = 11; mant_dig = 53; byte_order = native; align = 1; } mean;
		floating_point { exp_dig = 11; mant_dig = 53; byte_order = native; align = 1; } max;
		floating_point { exp_dig = 11; mant_dig = 53; byte_order = native; align = 1; } min;
		floating_point { exp_dig = 11; mant_dig = 53; byte_order = native; align = 1; } std;
		floating_point { exp_dig = 11; mant_dig = 53; byte_order = native; align = 1; } median;
		floating_point { exp_dig = 11; mant_dig = 53; byte_order = native; align = 1; } significant_digits;
	} align(1);
};

